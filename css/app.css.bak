.header-up
{  background: #0f7b3e;
  background: linear-gradient(to bottom, #0f7b3e, #152f18);
  color: #fff;
  font-size: 150%;
  padding: 10px 45px;
  height: 60px;
}
@media screen and (min-width:640px) and (max-width:1024px) {
  .header-up
  {
    font-size: 100%;
    padding: 8px 20px;
    height: 45px;
  }
  .header-up-right img
  {
    width: 20px;
  }
}
@media screen and (max-width:639px) {  .header-up
  {    font-size: 90%;
    padding: 8px 20px;
  }
  .header-up-right
  {    text-align: left;
  }
  .header-up-right img
  {    width: 20px;
  }
}

